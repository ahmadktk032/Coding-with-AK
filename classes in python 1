class Dog:
    def __init__(self,name,age):
        self.name = name
        self.age = age
    def bark(self):
        return f"{self.name} say Bark!"
    def get_info(self):
        return f"{self.name} is {self.age} years old"


dog1 = Dog("Buddy",6)
dog2 = Dog("Nia",7)
dog3 = Dog("lully",4)

print(dog1.bark())
print(dog2.bark())
print(dog3.bark())

print(dog1.get_info())
print(dog2.get_info())
print(dog3.get_info())
